def simple_file_check():
    filename = input("–í–≤–µ–¥–∏—Ç–µ –∏–º—è —Ñ–∞–π–ª–∞ —Å —Ä–∞—Å—à–∏—Ä–µ–Ω–∏–µ–º: ")
    if filename.endswith('.doc') or filename.endswith('.docx'):
        print("Word file")
    elif filename.endswith('.py'):
        print("Python file")
    elif filename.endswith('.txt'):
        print("Text file")
    else:
        if '.' in filename:
            extension = filename.split('.')[-1]
            print(f"–ù–µ–∏–∑–≤–µ—Å—Ç–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç —Ñ–∞–π–ª–∞ (.{extension})")
        else:
            print("–§–∞–π–ª –Ω–µ –∏–º–µ–µ—Ç —Ä–∞—Å—à–∏—Ä–µ–Ω–∏—è")number = float(input("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ: "))   
if number <= 0:  
    number = 1  
print(f"{number - 1}, {number}, {number + 1}")
#2
def simple_file_check():
    filename = input("–í–≤–µ–¥–∏—Ç–µ –∏–º—è —Ñ–∞–π–ª–∞ —Å —Ä–∞—Å—à–∏—Ä–µ–Ω–∏–µ–º: "
    if filename.endswith('.doc') or filename.endswith('.docx'):
        print("Word file")
    elif filename.endswith('.py'):
        print("Python file")
    elif filename.endswith('.txt'):
        print("Text file")
    else:
        if '.' in filename:
            extension = filename.split('.')[-1]
            print(f"–ù–µ–∏–∑–≤–µ—Å—Ç–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç —Ñ–∞–π–ª–∞ (.{extension})")
        else:
            print("–§–∞–π–ª –Ω–µ –∏–º–µ–µ—Ç —Ä–∞—Å—à–∏—Ä–µ–Ω–∏—è")
#3
def check_triangle_type():
    print("=== –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å —Ç–∏–ø–∞ —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞ ===")
    try:
        a = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –ø–µ—Ä–≤–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        b = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –≤—Ç–æ—Ä–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        c = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É —Ç—Ä–µ—Ç—å–µ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        if a <= 0 or b <= 0 or c <= 0:
            print("–û—à–∏–±–∫–∞: –≤—Å–µ —Å—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏ —á–∏—Å–ª–∞–º–∏!")
            return
        if (a + b <= c) or (a + c <= b) or (b + c <= a):
            print("–û—à–∏–±–∫–∞: —Ç–∞–∫–∏–µ —Å—Ç–æ—Ä–æ–Ω—ã –Ω–µ –º–æ–≥—É—Ç –æ–±—Ä–∞–∑–æ–≤–∞—Ç—å —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫!")
            print("(–°—É–º–º–∞ –ª—é–±—ã—Ö –¥–≤—É—Ö —Å—Ç–æ—Ä–æ–Ω –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å –±–æ–ª—å—à–µ —Ç—Ä–µ—Ç—å–µ–π)")
            return
        if a == b == c:
            print("–¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–í–ù–û–°–¢–û–†–û–ù–ù–ò–ô")
        elif a == b or a == c or b == c:
            print("–¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–í–ù–û–ë–ï–î–†–ï–ù–ù–´–ô")
        else:
            print("–¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–ó–ù–û–°–¢–û–†–û–ù–ù–ò–ô")
            
    except ValueError:
        print("–û—à–∏–±–∫–∞: –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è!")

def detailed_triangle_check():
    print("=== –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å —Ç–∏–ø–∞ —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞ ===")
    
    try:
        a = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –ø–µ—Ä–≤–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        b = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –≤—Ç–æ—Ä–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        c = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É —Ç—Ä–µ—Ç—å–µ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        if a <= 0 or b <= 0 or c <= 0:
            print("‚ùå –û—à–∏–±–∫–∞: –≤—Å–µ —Å—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏ —á–∏—Å–ª–∞–º–∏!")
            return
        sides_valid = True
        if a + b <= c:
            print(f"‚ùå –°—Ç–æ—Ä–æ–Ω—ã {a} + {b} = {a+b} ‚â§ {c} - –Ω–∞—Ä—É—à–µ–Ω–∏–µ –Ω–µ—Ä–∞–≤–µ–Ω—Å—Ç–≤–∞ —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞")
            sides_valid = False
        if a + c <= b:
            print(f"‚ùå –°—Ç–æ—Ä–æ–Ω—ã {a} + {c} = {a+c} ‚â§ {b} - –Ω–∞—Ä—É—à–µ–Ω–∏–µ –Ω–µ—Ä–∞–≤–µ–Ω—Å—Ç–≤–∞ —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞")
            sides_valid = False
        if b + c <= a:
            print(f"‚ùå –°—Ç–æ—Ä–æ–Ω—ã {b} + {c} = {b+c} ‚â§ {a} - –Ω–∞—Ä—É—à–µ–Ω–∏–µ –Ω–µ—Ä–∞–≤–µ–Ω—Å—Ç–≤–∞ —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞")
            sides_valid = False
        if not sides_valid:
            print("–¢–∞–∫–∏–µ —Å—Ç–æ—Ä–æ–Ω—ã –Ω–µ –º–æ–≥—É—Ç –æ–±—Ä–∞–∑–æ–≤–∞—Ç—å —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫!")
            return
        print(f"\n–°—Ç–æ—Ä–æ–Ω—ã —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞: {a}, {b}, {c}")
        if a == b == c:
            print("‚úÖ –¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–í–ù–û–°–¢–û–†–û–ù–ù–ò–ô")
            print("   (–í—Å–µ —Ç—Ä–∏ —Å—Ç–æ—Ä–æ–Ω—ã —Ä–∞–≤–Ω—ã)")
        elif a == b:
            print("‚úÖ –¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–í–ù–û–ë–ï–î–†–ï–ù–ù–´–ô")
            print(f"   (–°—Ç–æ—Ä–æ–Ω—ã a={a} –∏ b={b} —Ä–∞–≤–Ω—ã, –æ—Å–Ω–æ–≤–∞–Ω–∏–µ c={c})")
        elif a == c:
            print("‚úÖ –¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–í–ù–û–ë–ï–î–†–ï–ù–ù–´–ô")
            print(f"   (–°—Ç–æ—Ä–æ–Ω—ã a={a} –∏ c={c} —Ä–∞–≤–Ω—ã, –æ—Å–Ω–æ–≤–∞–Ω–∏–µ b={b})")
        elif b == c:
            print("‚úÖ –¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–í–ù–û–ë–ï–î–†–ï–ù–ù–´–ô")
            print(f"   (–°—Ç–æ—Ä–æ–Ω—ã b={b} –∏ c={c} —Ä–∞–≤–Ω—ã, –æ—Å–Ω–æ–≤–∞–Ω–∏–µ a={a})")
        else:
            print("‚úÖ –¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–ó–ù–û–°–¢–û–†–û–ù–ù–ò–ô")
            print("   (–í—Å–µ —Å—Ç–æ—Ä–æ–Ω—ã –∏–º–µ—é—Ç —Ä–∞–∑–Ω—É—é –¥–ª–∏–Ω—É)")
    except ValueError:
        print("‚ùå –û—à–∏–±–∫–∞: –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è!")
def triangle_type_detector():
    print("=== –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å —Ç–∏–ø–∞ —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞ ===")
    print("–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤–≤–µ–¥–∏—Ç–µ 'exit'\n")
    while True:
        try:
            user_input = input("–í–≤–µ–¥–∏—Ç–µ —Ç—Ä–∏ —Å—Ç–æ—Ä–æ–Ω—ã —á–µ—Ä–µ–∑ –ø—Ä–æ–±–µ–ª: ").strip()
            if user_input.lower() == 'exit':
                print("–î–æ —Å–≤–∏–¥–∞–Ω–∏—è!")
                break
            if not user_input:
                continue
            sides = user_input.split()
            if len(sides) != 3:
                print("‚ùå –û—à–∏–±–∫–∞: –Ω—É–∂–Ω–æ –≤–≤–µ—Å—Ç–∏ —Ä–æ–≤–Ω–æ —Ç—Ä–∏ —á–∏—Å–ª–∞!")
                continue
            a = float(sides[0])
            b = float(sides[1])
            c = float(sides[2])
            if a <= 0 or b <= 0 or c <= 0:
                print("‚ùå –û—à–∏–±–∫–∞: –≤—Å–µ —Å—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏!")
                continue
            if (a + b <= c) or (a + c <= b) or (b + c <= a):
                print("‚ùå –û—à–∏–±–∫–∞: —Ç–∞–∫–∏–µ —Å—Ç–æ—Ä–æ–Ω—ã –Ω–µ –º–æ–≥—É—Ç –æ–±—Ä–∞–∑–æ–≤–∞—Ç—å —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫!")
                continue
            if a == b == c:
                print("üìê –¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–í–ù–û–°–¢–û–†–û–ù–ù–ò–ô")
            elif a == b or a == c or b == c:
                print("üìê –¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–í–ù–û–ë–ï–î–†–ï–ù–ù–´–ô")
            else:
                print("üìê –¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫: –†–ê–ó–ù–û–°–¢–û–†–û–ù–ù–ò–ô")
            print() 
        except ValueError:
            print("‚ùå –û—à–∏–±–∫–∞: –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è!")
        except KeyboardInterrupt:
            print("\n\n–ü—Ä–æ–≥—Ä–∞–º–º–∞ –∑–∞–≤–µ—Ä—à–µ–Ω–∞.")
            break
if __name__ == "__main__":
    check_triangle_type()
    detailed_triangle_check()
    triangle_type_detector()
–í–∞—Ä–∏–∞–Ω—Ç 4
#1
text = 'important information in one line'
letter = input("–í–≤–µ–¥–∏—Ç–µ –±—É–∫–≤—É: ").strip().lower()  
if len(letter) == 1:  # –ø—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –≤–≤–µ–¥–µ–Ω–æ –∏–º–µ–Ω–Ω–æ –æ–¥–Ω–∞ –±—É–∫–≤–∞  
    result = letter in text  
    print(result)  
else:  
    print("–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —Ä–æ–≤–Ω–æ –æ–¥–Ω—É –±—É–∫–≤—É") 
#2
def calculate_figure():
    print("=== –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å —Ñ–∏–≥—É—Ä—ã –∏ –ø–ª–æ—â–∞–¥–∏ ===")
    
    try:
        side1 = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –ø–µ—Ä–≤–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        side2 = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –≤—Ç–æ—Ä–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        if side1 <= 0 or side2 <= 0:
            print("‚ùå –û—à–∏–±–∫–∞: —Å—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏ —á–∏—Å–ª–∞–º–∏!")
            return
        if side1 == side2:
            figure_type = "–ö–í–ê–î–†–ê–¢"
        else:
            figure_type = "–ü–†–Ø–ú–û–£–ì–û–õ–¨–ù–ò–ö"
        area = side1 * side2
        print(f"\nüìê –§–∏–≥—É—Ä–∞: {figure_type}")
        print(f"üìè –°—Ç–æ—Ä–æ–Ω–∞ A: {side1}")
        print(f"üìè –°—Ç–æ—Ä–æ–Ω–∞ B: {side2}")
        print(f"üìä –ü–ª–æ—â–∞–¥—å: {area}")
        
    except ValueError:
        print("‚ùå –û—à–∏–±–∫–∞: –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è!")
def detailed_figure_calculation():
    print("=== –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å —Ñ–∏–≥—É—Ä—ã –∏ –ø–ª–æ—â–∞–¥–∏ ===")
    
    try:
        a = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –ø–µ—Ä–≤–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        b = float(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –≤—Ç–æ—Ä–æ–π —Å—Ç–æ—Ä–æ–Ω—ã: "))
        
        if a <= 0 or b <= 0:
            print("‚ùå –û—à–∏–±–∫–∞: —Å—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏ —á–∏—Å–ª–∞–º–∏!")
            return
        if a == b:
            figure_type = "–ö–í–ê–î–†–ê–¢"
            description = f"–í—Å–µ —Å—Ç–æ—Ä–æ–Ω—ã —Ä–∞–≤–Ω—ã ({a})"
        else:
            figure_type = "–ü–†–Ø–ú–û–£–ì–û–õ–¨–ù–ò–ö"
            description = f"–°–º–µ–∂–Ω—ã–µ —Å—Ç–æ—Ä–æ–Ω—ã: {a} –∏ {b}"
        area = a * b
        perimeter = 2 * (a + b)
        print(f"\n{'='*40}")
        print(f"üìê –§–ò–ì–£–†–ê: {figure_type}")
        print(f"{'='*40}")
        print(f"üìè –°—Ç–æ—Ä–æ–Ω–∞ A: {a}")
        print(f"üìè –°—Ç–æ—Ä–æ–Ω–∞ B: {b}")
        print(f"üìê –•–∞—Ä–∞–∫—Ç–µ—Ä–∏—Å—Ç–∏–∫–∞: {description}")
        print(f"üìä –ü–ª–æ—â–∞–¥—å: {area}")
        print(f"üìè –ü–µ—Ä–∏–º–µ—Ç—Ä: {perimeter}")
        if figure_type == "–ö–í–ê–î–†–ê–¢":
            print(f"üí° –î–∏–∞–≥–æ–Ω–∞–ª—å: {a * (2**0.5):.2f}")
    except ValueError:
        print("‚ùå –û—à–∏–±–∫–∞: –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è!")
def interactive_figure_detector():
    print("=== –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å —Ñ–∏–≥—É—Ä –∏ –ø–ª–æ—â–∞–¥–µ–π ===")
    print("–î–ª—è –≤—ã—Ö–æ–¥–∞ –≤–≤–µ–¥–∏—Ç–µ 'exit'\n")
    while True:
        try:
            user_input = input("–í–≤–µ–¥–∏—Ç–µ –¥–≤–µ —Å—Ç–æ—Ä–æ–Ω—ã —á–µ—Ä–µ–∑ –ø—Ä–æ–±–µ–ª: ").strip()
            if user_input.lower() == 'exit':
                print("–î–æ —Å–≤–∏–¥–∞–Ω–∏—è!")
                break
            if not user_input:
                continue
            sides = user_input.split()
            if len(sides) != 2:
                print("‚ùå –û—à–∏–±–∫–∞: –Ω—É–∂–Ω–æ –≤–≤–µ—Å—Ç–∏ —Ä–æ–≤–Ω–æ –¥–≤–∞ —á–∏—Å–ª–∞!")
                continue
            a = float(sides[0])
            b = float(sides[1])
            if a <= 0 or b <= 0:
                print("‚ùå –û—à–∏–±–∫–∞: —Å—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏!")
                continue
            if a == b:
                figure = "–ö–í–ê–î–†–ê–¢"
                emoji = "‚¨ú"
            else:
                figure = "–ü–†–Ø–ú–û–£–ì–û–õ–¨–ù–ò–ö"
                emoji = "üü¶"
            area = a * b
            perimeter = 2 * (a + b)
            print(f"{emoji} –§–∏–≥—É—Ä–∞: {figure}")
            print(f"   –°—Ç–æ—Ä–æ–Ω—ã: {a} √ó {b}")
            print(f"   –ü–ª–æ—â–∞–¥—å: {area}")
            print(f"   –ü–µ—Ä–∏–º–µ—Ç—Ä: {perimeter}")
            print()
        except ValueError:
            print("‚ùå –û—à–∏–±–∫–∞: –ø–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è!")
        except KeyboardInterrupt:
            print("\n\n–ü—Ä–æ–≥—Ä–∞–º–º–∞ –∑–∞–≤–µ—Ä—à–µ–Ω–∞.")
            break
def simple_figure_check():
    try:
        a = float(input("–í–≤–µ–¥–∏—Ç–µ –ø–µ—Ä–≤—É—é —Å—Ç–æ—Ä–æ–Ω—É: "))
        b = float(input("–í–≤–µ–¥–∏—Ç–µ –≤—Ç–æ—Ä—É—é —Å—Ç–æ—Ä–æ–Ω—É: "))        
        if a <= 0 or b <= 0:
            print("–°—Ç–æ—Ä–æ–Ω—ã –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã–º–∏!")
            return
        if a == b:
            print(f"–ö–≤–∞–¥—Ä–∞—Ç —Å–æ —Å—Ç–æ—Ä–æ–Ω–æ–π {a}, –ø–ª–æ—â–∞–¥—å = {a*b}")
        else:
            print(f"–ü—Ä—è–º–æ—É–≥–æ–ª—å–Ω–∏–∫ {a}√ó{b}, –ø–ª–æ—â–∞–¥—å = {a*b}")
    except ValueError:
        print("–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞!")
if __name__ == "__main__":
    calculate_figure()
    detailed_figure_calculation()
    interactive_figure_detector()
    simple_figure_check()
#3
def check_mood(response):   
    positive = ['—Ö–æ—Ä–æ—à–æ', '–Ω–æ—Ä–º–∞–ª—å–Ω–æ', '–æ—Ç–ª–∏—á–Ω–æ']   
    negative = ['–ø–ª–æ—Ö–æ', '–Ω–µ —Ö–æ—Ä–æ—à–æ']   
    response = response.lower()  
    if response in positive:  
        return 'üòä'  
    elif response in negative:  
        return 'üôÅ'  
    else:  
        return 'üòê' 
–í–∞—Ä–∏–∞–Ω—Ç 5
#1


num1 = float(input("–í–≤–µ–¥–∏—Ç–µ –ø–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ: "))
num2 = float(input("–í–≤–µ–¥–∏—Ç–µ –≤—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ: "))
if num1 > num2:
    result = num1 ** num2
    print(f"–ü–µ—Ä–≤–æ–µ —á–∏—Å–ª–æ ({num1}) –±–æ–ª—å—à–µ. –†–µ–∑—É–ª—å—Ç–∞—Ç –≤–æ–∑–≤–µ–¥–µ–Ω–∏—è –≤ —Å—Ç–µ–ø–µ–Ω—å: {result}")
 elif num2 > num1:
     result = num2 ** num1
    print(f"–í—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ ({num2}) –±–æ–ª—å—à–µ. –†–µ–∑—É–ª—å—Ç–∞—Ç –≤–æ–∑–≤–µ–¥–µ–Ω–∏—è –≤ —Å—Ç–µ–ø–µ–Ω—å: {result}")
else:
    result = num1 + num2
    print(f"–ß–∏—Å–ª–∞ —Ä–∞–≤–Ω—ã ({num1} –∏ {num2}). –ò—Ö —Å—É–º–º–∞: {result}")
    print("–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞. –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ –∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–µ —á–∏—Å–ª–∞.")
#2
new_message = "Hello! How are you?"
response = input("–í–≤–µ–¥–∏—Ç–µ –≤–∞—à –æ—Ç–≤–µ—Ç –Ω–∞ —Å–æ–æ–±—â–µ–Ω–∏–µ 'Hello! How are you?': ")
if new_message and response:
    if new_message[0] == response[0]:
        print(True)
    else:
        print(False)
else:
    print("–û–¥–Ω–∞ –∏–∑ —Å—Ç—Ä–æ–∫ –ø—É—Å—Ç–∞, —Å—Ä–∞–≤–Ω–µ–Ω–∏–µ –Ω–µ–≤–æ–∑–º–æ–∂–Ω–æ.")
