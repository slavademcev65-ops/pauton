bin_sy = ['11011111', '11011101', '11000111', '11011100', '11011110']  
dec_sy =   
for binary in bin_sy:  
    decimal = int(binary, 2)
    dec_sy.append(decimal)  
    print(f"Двоичное число {binary} в десятичной системе: {decimal}")  
max_value = max(dec_sy)  
min_value = min(dec_sy)    
print(f"\nМаксимальное число: {max_value}")  
print(f"Минимальное число: {min_value}")  
nt('Введите корректное число')   

Задание 3

bin_sy = ['11011111', '11011101', '11000111', '11011100', '11011110']  
dec_sy =   
for bin_num in bin_sy:  
    dec_num = int(bin_num,
    dec_sy.append(dec_num)  
    print(f"Двоичное число {bin_num} в десятичной системе: {dec_num}")    
max_num = max(dec_sy)  
min_num = min(dec_sy)  
print(f"\nМаксимальное число: {max_num}")  
print(f"Минимальное число: {min_num}")

Задание 4
A = [  
    [-446, 281, -804],  
    [65, 432, -122],  
    [13, "error", 8]  
]  
rows = len(A)  
cols = len(A)  
for i, row in enumerate(A):  
    for j, element in enumerate(row):  
        if isinstance(element, str):   
            A[i][j] = len(element)    
print("Исправленная матрица:")  
for row in A:  
    print(row) 

